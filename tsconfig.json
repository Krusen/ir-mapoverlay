        "app/components/settings.ts",
        "app/map-overlay.ts",
        "libs/typings/bootstrap/bootstrap.d.ts",
        "libs/typings/jquery/jquery.d.ts",
        "libs/typings/lodash/lodash.d.ts",
        "libs/typings/tsd.d.ts"
    ],
    "exclude": []
}
